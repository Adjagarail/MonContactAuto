{% extends 'admin.html.twig' %}

{% block title %}Convoyage{% endblock %}

{% block body %}
    <h1>Convoyage</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ convoyage.id }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ convoyage.prenom }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ convoyage.nom }}</td>
            </tr>
            <tr>
                <th>Villed</th>
                <td>{{ convoyage.villed }}</td>
            </tr>
            <tr>
                <th>Villea</th>
                <td>{{ convoyage.villea }}</td>
            </tr>
            <tr>
                <th>CalendrierAt</th>
                <td>{{ convoyage.calendrierAt ? convoyage.calendrierAt|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Telephone</th>
                <td>{{ convoyage.telephone }}</td>
            </tr>
            <tr>
                <th>Marque</th>
                <td>{{ convoyage.marque }}</td>
            </tr>
            <tr>
                <th>Modele</th>
                <td>{{ convoyage.modele }}</td>
            </tr>
        </tbody>
    </table>



    <a href="{{ path('convoyages_edit', {'id': convoyage.id}) }}">edit</a>

    {{ include('convoyages/_delete_form.html.twig') }}
{% endblock %}
