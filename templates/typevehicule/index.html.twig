{% extends 'admin.html.twig' %}

{% block title %}Typevehicule index{% endblock %}

{% block body %}

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for typevehicule in typevehicules %}
            <tr>
                <td>{{ typevehicule.id }}</td>
                <td>{{ typevehicule.name }}</td>
                <td>
                    <a href="{{ path('typevehicule_show', {'id': typevehicule.id}) }}">show</a>
                    <a href="{{ path('typevehicule_edit', {'id': typevehicule.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('typevehicule_new') }}">Create new</a>
{% endblock %}
